{
  "name": "koa2-deploy-server",
  "version": "1.0.0",
  "description": "项目发布后台",
  "main": "app.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --harmony ./src/app.js --config nodemonrc.json",
    "auto:sql": "NODE_ENV=staging node ./src/db/auto/index.js",
    "test:sequelize": "NODE_ENV=staging node ./src/model/mysql/authenticate.js",
    "test:mongoose": "NODE_ENV=staging node ./src/model/mongo/authenticate.js",
    "deploy:stag": "NODE_ENV=staging pm2 deploy ecosystem.config.js staging",
    "deploy:prod": "NODE_ENV=production  pm2 deploy ecosystem.config.js production"
  },
  "keywords": [
    "发布",
    "记录"
  ],
  "author": "yunaichun",
  "license": "ISC",
  "dependencies": {
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-mysql-session": "0.0.2",
    "koa-router": "^10.0.0",
    "koa-session-minimal": "^4.0.0",
    "koa-static": "^5.0.0",
    "mongoose": "^5.10.14",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
